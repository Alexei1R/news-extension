{
  "manifest_version": 3,
  "name": "News Extension",
  "version": "1.0.0",
  "description": "A simple news extension",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "news-extension@example.com"
    }
  }
}
